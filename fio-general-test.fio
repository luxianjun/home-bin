#
#User guide: https://fio.readthedocs.io/en/latest/fio_doc.html
#
# bs: 4k, 8k, 64k, 1M, 4M
# usage: fio fio-general-test.fio
#

[global]
ramp_time=10
runtime=60
size=10g
ioengine=libaio
iodepth=4
numjobs=$ncpus
bsrange=$pagesize-4m
filename=/dev/sdb1

[bsrange_randread_direct]
direct=1
rw=randread

[bsrange_randread]
rw=randread

[bsrange_read_write_direct]
direct=1
rw=randrw

[bsrange_read_write]
rw=randrw

[bsrange_randwrite_direct]
direct=1
rw=randwrite

[bsrange_randwrite]
rw=randwrite

[bsrange_read_direct]
rw=read
direct=1

[bsrange_read]
rw=read

[bsrange_write_direct]
direct=1
rw=write

[bsrange_write]
rw=write


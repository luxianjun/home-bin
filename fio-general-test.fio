#
#User guide: https://fio.readthedocs.io/en/latest/fio_doc.html
#
# bs: 4k, 8k, 64k, 1M, 4M
# usage: fio thisfile 2>&1|tee fio-log.txt
# HWE32SS3008M001N sas ssd 4k,8k,64k,1m,4m test
#
#

[global]
ramp_time=10
runtime=43200
#size=10g
ioengine=libaio
iodepth=4
#numjobs=$ncpus
filename=/dev/sdd

[range4k-4m_randread_direct]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
direct=1
rw=randread

[range4k-4m_randread]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
rw=randread

[range4k-4m_read_write_direct]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
direct=1
rw=randrw

[range4k-4m_read_write]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
rw=randrw

[range4k-4m_randwrite_direct]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
direct=1
rw=randwrite

[range4k-4m_randwrite]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
rw=randwrite

[range4k-4m_read_direct]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
rw=read
direct=1

[range4k-4m_read]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
rw=read

[range4k-4m_write_direct]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
direct=1
rw=write

[range4k-4m_write]
bsrange=4k-4m
bssplit=4k/20:8k/20:64k/20:1m/20:4m/20
rw=write

